package kaisen ;
import java.util.Scanner
public class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);

        int nomor, akhir;
        System.out.println("pastikan awalan nomor lebih kecil dari akhiran");

        System.out.println("masukkan nomor awalan anda");
        System.out.println();
        nomor = input.nextInt();

        System.out.println("masukkan akhiran nomor");
        System.out.println();
        akhir = input.nextInt();
        System.out.println();

        for (int a=nomor ; a<=akhir;a--){
        System.out.println(a);
        }
        
    }
    
}
