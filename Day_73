package Social;

class OuterClass {
  String namaBelakang = "Nur ";
  class InnerClass {
      String namaDepan = "Rahiman";
  }
}
public class Main {
  public static void main(String[] args) {
    OuterClass myOuter = new OuterClass();
    OuterClass.InnerClass myInner = myOuter.new InnerClass();
    System.out.println(myInner.namaDepan +' '+ myOuter.namaBelakang);
  }
}
