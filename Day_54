package kaisen;
import java.util.Scanner;
public class Main {
    static double hitungLuas(double alas, double tinggi) {
    double luas = (alas * tinggi) / 2;
    return luas;
  }
   
  public static void main(String args[]){
    double hasil = hitungLuas(2, 3);
    System.out.println(hasil);
  }
}
