package linkedlist1;
import java.util.LinkedList;
public class LinkedList1 {

    public static void main(String[] args) {
        
        LinkedList<String> Mobil2 = new LinkedList<String>();
            Mobil2.add("Toyota");
            Mobil2.add("Daihatsu");
            Mobil2.add("Isuzu");
            Mobil2.add("Hyundai");
            Mobil2.add("Daihatsu");
            Mobil2.add("Honda");
            
            if(Mobil2.isEmpty()){
        System.out.println("Index LinkedList tak bernilai.");
            }else{System.out.println("Nilai index LinkedList : "+Mobil2);}
    }
    
}
