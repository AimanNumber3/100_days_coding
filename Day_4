package aiman;

import java.util.Scanner;

public class Main {
    
    public static void main(String[] args){ 
        
        Scanner input = new Scanner (System.in);
    
        System.out.println("apa nama depan anda ? ");
    										String depan = input.nextLine();
        
        System.out.println("apa nama belakang anda ? ");
    										String belakang = input.nextLine();
        
        System.out.println();
        System.out.println("halo "+depan+' '+belakang+'!');
        System.out.println("senang bertemu denganmu!!");
        
    }
    
}
