package linkedlist1;
import java.util.LinkedList;
public class LinkedList1 {

    public static void main(String[] args) {
        
        LinkedList<String> Mobil2 = new LinkedList<String>();
            Mobil2.add("Toyota");
            Mobil2.add("Tesla");
            Mobil2.add("Isuzu");
            Mobil2.add("Hyundai");
            Mobil2.add("Daihatsu");
            Mobil2.add("Honda");
            System.out.println("Index LinkedList sebelum perintah (.poll) : \n"+Mobil2+'\n');

            String Po1 = Mobil2.poll();
            String Po2 = Mobil2.pollFirst();
            String Po3 = Mobil2.pollLast();
            System.out.println("Hasil setelah poll : \n"+Mobil2);
    }
    
}
