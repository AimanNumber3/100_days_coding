package linkedlist1;
import java.util.LinkedList;
public class LinkedList1 {

    public static void main(String[] args) {
        
        LinkedList<LinkedList<String>> Daftar2 = new LinkedList();
        
        LinkedList<String> Bahan2 = new LinkedList<String>();
           Bahan2.add ("tomat");
           Bahan2.add ("cabai");
           Bahan2.add ("bawang");
           
        LinkedList<String> Alat2 = new LinkedList<String>();
            Alat2.add ("wajan");
            Alat2.add ("blender");
            Alat2.add ("spatula");
            
            Daftar2.add(Bahan2);
            Daftar2.add(Alat2);
            System.out.println("Isi nilai LinkedList : \n"+Daftar2+" \n");
            
            int ukuran = Daftar2.size();
            System.out.println("Ukuran LinkedList : "+ukuran);
    }
    
}
